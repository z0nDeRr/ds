#include <iostream>
#include <string> 
using namespace std;

int main()
{
    int i = 0;
    string s;
    cout << "String:\n";
    getline(cin, s);
    s = ' ' + s;
    while (s[i] != '\0')
    {
        if (s[i] == ' ' && s[i + 1] != ' ') s.erase(i + 1, 1);
        i++;
    }
    s.erase(0, 1);
    cout << s << endl;
    system("pause");
    return 0;
}
